{
  "name": "virtual-fashion-photographer",
  "version": "1.0.0",
  "description": "Generate high-end fashion photography from text descriptions and edit existing images with AI.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "build": "esbuild src/index.tsx --bundle --outfile=dist/index.js --loader:.ts=tsx --jsx=automatic --define:process.env.NODE_ENV='\"production\"' && cp public/index.html dist/index.html",
    "dev": "npm run build && node server.js"
  },
  "dependencies": {
    "@google/genai": "^1.26.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.4",
    "typescript": "^5.4.5"
  }
}
